    "dependencies": {
        "@koa/router": "^9.1.0",
        "@peregrine/koa-with-decorators": "^0.1.1",
        "axios": "^0.21.1",
        "dotenv": "^8.2.0",
        "koa": "^2.13.0",
        "koa-bodyparser": "^4.3.0",
        "moment": "^2.27.0"
    },
    "devDependencies": {
